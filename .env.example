# Point API Configuration
# Copy this file to .env and fill in your actual values

# Your Point API key (required)
POINT_API_KEY=your_api_key_here_12345abcdef

# Your organization identifier (required)
POINT_ORGANIZATION_ID=603

# Distribution type ID (optional, defaults to "1")
POINT_DISTRIBUTION_TYPE_ID=1

# Development/Testing Configuration
# Set to true for development mode
DEBUG=false

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# API timeout in seconds
API_TIMEOUT=30

# Maximum retries for API calls
MAX_RETRIES=3